{
  "name": "vite-svelte-ts-starter",
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "svelte-check",
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "prettier": "prettier --write",
    "lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,ts,svelte}\"",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,svelte}\"",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.35",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.4",
    "daisyui": "^2.13.6",
    "eslint-plugin-svelte3": "^3.4.0",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.2",
    "svelte-routing": "^1.6.0",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  }
}
